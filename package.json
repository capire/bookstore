{
  "name": "@capire/bookstore",
  "version": "2.0.1",
  "description": "Shows how to reuse and compose with CAP.",
  "repository": "https://github.com/capire/bookstore",
  "dependencies": {
    "@cap-js/hana": ">=1",
    "@capire/bookshop": "*",
    "@capire/common": "*",
    "@capire/data-viewer": "*",
    "@capire/orders": "*",
    "@capire/reviews": "*",
    "@sap-cloud-sdk/http-client": "^4",
    "@sap-cloud-sdk/resilience": "^4",
    "@sap/cds": ">=5"
  },
  "devDependencies": {
    "@cap-js/cds-test": "^0.4.1",
    "@cap-js/sqlite": ">=1"
  },
  "scripts": {
    "start": "cds-serve",
    "watch": "cds watch",
    "chest": "chest",
    "jest": "jest",
    "test": "chest"
  },
  "cds": {
    "requires": {
      "ReviewsService": "odata",
      "OrdersService": "odata",
      "messaging": true,
      "db": {
        "[development]": {
          "model": "db/sqlite"
        },
        "[production]": {
          "model": "db/hana"
        }
      }
    },
    "log": {
      "service": true
    }
  }
}
